{
  "name": "@mkholt/pdf-thumbnail",
  "version": "1.0.3",
  "description": "React wrapper around PDF-js allowing creation of thumbnails both server and client side",
  "main": "dist/index.js",
  "engines": {
    "node": "^18.12.0 || >=20.9.0"
  },
  "repository": {
    "type": "git",
    "url": "github:mkholt/pdf-thumbnail"
  },
  "files": [
    "dist/*"
  ],
  "author": "Morten Holt <mho@delegate.dk>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "preversion": "yarn test run",
    "prepack": "yarn build",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "postversion": "git push --tags && x-var yarn publish --access public --new-version $npm_package_version . && git push",
    "pretest": "yarn type-check",
    "test": "vitest",
    "pretest:coverage": "yarn type-check",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24",
    "@types/react": "^18.3.11",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4",
    "@vitest/ui": "^4",
    "canvas": "^3.0.1",
    "jsdom": "^27.4.0",
    "msw": "^2.4.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.2",
    "vitest": "^4",
    "x-var": "^3.0.1"
  },
  "peerDependencies": {
    "canvas": ">=3.x",
    "react": ">=16.8.0"
  },
  "dependencies": {
    "@mkholt/utilities": "^1.0.2",
    "pdfjs-dist": "5"
  }
}
